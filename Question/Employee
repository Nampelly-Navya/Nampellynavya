package in.com.spring;

public class Employee1
{
 int id;
 String name;
 int salary;
 String dept;
public Employee1(int id, String name, int salary, String dept) {
	super();
	this.id = id;
	this.name = name;
	this.salary = salary;
	this.dept = dept;
}
@Override
public String toString() {
	return "Employee1 [id=" + id + ", name=" + name + ", salary=" + salary + ", dept=" + dept + "]";
}
 
}
